  <select name='type' id="select" onChange={chName(this.id)} >
    <option selected='selected'>string</option>
    <option>number</option>
    <option>array</option>
  </select><br id = 'br_3 '>
  <label id = "descrlabel">Default:</label><br id = 'br_4'>
  <input type="text" id="descrinput" name="descrinput" ><br id = 'br_5'>
  <label id = "description label" >Description:</label><br id = 'br_6'>
  <input type="text" id="description" name="description" ><br id = 'br_7'>
  <br id = 'br_8' >
  <br id = 'br_9'>
</div>
</form>
<button id = "add" class="button-55">Add New</button>
<button id = "confirm" form = "editForm" class="button-55">Submit</button>
</div>
</div>

